<mujoco model="example">
    <compiler angle="degree" meshdir="meshes" coordinate="local" inertiafromgeom="true"/>
    <default>
        <joint damping='10' armature='0.1' limited="true" stiffness="8"/>
        <geom conaffinity="0" condim="3" contype="1" friction="1 1 1" solimp="0.0 0.8 0.01"
              solref="0.02 1"/>
        <!--        <motor ctrllimited="true" ctrlrange="-1 1"/>-->
    </default>
    <size nstack="300000" nuser_geom="1"/>
    <option gravity="0 0 -9.81" timestep="0.01"/>

    <asset>
        <mesh file="base_in_feet.STL"/>
        <mesh file="middle_in_feet.STL"/>
        <mesh file="top_in_feet.STL"/>
        <texture name='plane' type='2d' builtin='checker' rgb1='0.1 0.1 0.1' rgb2='0.2 0.2 0.2' width='128'
                 height='128'/>

        <material name='floor_mat' reflectance='0.0' texture='plane' texrepeat='5 5' texuniform='true'/>
    </asset>

    <worldbody>
        <geom name="floor0" pos="0 0 -0.33" size='1 1 1' type="plane" condim="3" conaffinity='15' material="floor_mat"/>
        <light directional='true' pos='0 0 3' dir='0 0 -1' diffuse='0.8 0.8 0.8' specular='0 0 0'/>

        <body name="base" pos="0 0 0">
            <!--            <joint name="joint0" axis="1 0 0" pos="0 0 0" range="1.57 4.71" type="hinge"/>-->
            <geom name="link0" type="mesh" mesh="base_in_feet" pos="0 0 0"/>
            <inertial pos="0 0 0" mass="0.1" diaginertia="0.3 0.3 0.3"/>

            <body name="middle" pos="0 0 0">
                <joint name="joint2" axis="1 0 0" pos="0 0 0" range="-90 90" type="hinge"/>-->

                <geom name="link1" type="mesh" mesh="middle_in_feet" pos="0 0 0" euler="0 0 0"/>
                <inertial pos="0 0 0" mass="0.1" diaginertia="0.3 0.3 0.3"/>

                <body name="top" pos="0.0656 -0.07546 0.0656">
                    <joint name="joint3" axis="0 1 0" pos="0 0 0" range="-90 90" type="hinge"/>

                    <geom name="link2" type="mesh" mesh="top_in_feet" pos="0 0 0" euler="0 0 0"/>
                    <inertial pos="0 0 0" mass="0.1" diaginertia="0.3 0.3 0.3"/>

                </body>

            </body>
        </body>
    </worldbody>

    <actuator>
        <!--        <position ctrllimited="false"  ctrlrange="0 6.283185308" gear="1" name="joint0" joint="joint0"/>-->
        <!--        <motor gear="5000" name="joint1" joint="joint1"/>-->
        <!--        <position ctrllimited="true" gear="1" ctrlrange="0 30" name="joint2" joint="joint2"/>-->
        <position ctrllimited="true" ctrlrange="-90 90" gear="1" name="joint2" joint="joint2"/>
        <motor ctrllimited="true" ctrlrange="-90 90" gear="1" name="joint3" joint="joint3"/>
        <!--        <position ctrllimited="true" ctrlrange="-3.14 3.14" gear="1"  name="joint3" joint="joint3"/>-->
        <!--        <position gear="1" name="joint0" joint="joint0"/>-->
    </actuator>

</mujoco>